  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IonicaBizau/ssh-remote.git"
  },
  "keywords": [
    "ssh",
    "remote",
    "git",
    "update"
  ],
  "author": "Ionică Bizău <bizauionica@gmail.com> (http://ionicabizau.net)",
  "license": "MIT",
  "bugs": {
